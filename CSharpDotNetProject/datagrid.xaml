<Window x:Class="CSharpDotNetProject.datagrid"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:CSharpDotNetProject"
        mc:Ignorable="d"
        Title="datagrid" Height="450" Width="800">
    <Grid x:Name="text2">
        <DataGrid Margin="0,24,0,27" Name="dtGrid" ItemsSource="{Binding}"/>
        <Button x:Name="save_button" Content="Save" HorizontalAlignment="Left" Margin="375,266,0,0" VerticalAlignment="Top" Height="60" Width="93" Click="save_button_Click"/>
        <TextBox x:Name="Text1" HorizontalAlignment="Left" Margin="328,196,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="Text2" HorizontalAlignment="Left" Margin="328,231,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
    </Grid>
</Window>
